<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p> <%= homeStartingContent %> </p>
    <% posts.forEach(function(post){ %>
        <h1> <%= post.postTitle %> </h1>
        <p> 
            <%= post.postBody.substring(0,100) + " ..." %> 
            <a href=<%= post.postLink %> > Read More </a>
        </p>
        
    <% }); %>

<%- include("partials/footer"); -%>